<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Pojo.Admin" table="admin">
      <id column="AdminId" name="AdminId" type="int">
      <generator class="native">
          
      </generator>
      
  </id>
  
  <property name="AdminName">
      <column name="AdminName"></column>
  </property>
  <property name="AdminSurname">
      <column name="AdminSurname"></column>
  </property>

  <property name="Type">
      <column name="Type"></column>

  <property name="Username">
      <column name="Username"></column>
  </property>
  <property name="Password">
      <column name="Password"></column>
  </property>

    <property name="ConfirmPassword">
    <column name="ConfirmPassword"></column>
  </property>
  
  </class>
</hibernate-mapping>

