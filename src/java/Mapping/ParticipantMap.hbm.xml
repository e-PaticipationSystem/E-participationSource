<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Pojo.Participant" table="participants">
      <id column="ParticipantId" name="ParticipantId" type="int">
      <generator class="native">
          
      </generator>
      
  </id>
  
  <property name="ParticipantName">
      <column name="ParticipantName"></column>
  </property>
  <property name="ParticipantSurname">
      <column name="ParticipantSurname"></column>
  </property>
  
  </class>
  
</hibernate-mapping>
