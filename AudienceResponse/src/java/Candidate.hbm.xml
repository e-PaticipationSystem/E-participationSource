<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.Pojo.Candidate" table="candidate">
       <id column="CandidateId" name="CandidateId" type="int">
      <generator class="native">
          
      </generator>
      
  </id>
  
  <property name="CandidateName">
      <column name="CandidateName"></column>
  </property>
  <property name="Result">
      <column name="Result"></column>
  </property>
  
  <property name="BallotId">
      <column name="BallotId"></column>
  </property>
  </class>
</hibernate-mapping>
